<?xml version="1.0" encoding="UTF-8"?>
<ProgramOp>
  <VarDeclList>
    <VarDeclOp>
      <TypeOp>int</TypeOp>
      <IdListInitOp>
        <IdList>
          <SimpleAssignOp>
            <id>n</id>
            <IntConst>0</IntConst>
          </SimpleAssignOp>
        </IdList>
      </IdListInitOp>
    </VarDeclOp>
  </VarDeclList>
  <ProcOpList>
    <ProcOp>
      <id>main</id>
      <ParamDeclList>null</ParamDeclList>
      <ResultTypeList>
        <ResultTypeOp>void</ResultTypeOp>
      </ResultTypeList>
      <ProcBodyOp>
        <VarDeclList>null</VarDeclList>
        <StatList>
          <WhileOp>
            <StatList>
              <WriteOp>
                <ExprList>
                  <StringConst>Enter n, or &gt;= 10 to exit:</StringConst>
                </ExprList>
              </WriteOp>
              <ReadOp>
                <IdList>
                  <id>n</id>
                </IdList>
              </ReadOp>
            </StatList>
            <LtOp>
              <id>n</id>
              <IntConst>10</IntConst>
            </LtOp>
            <DoOp>
              <StatList>
                <CallProcOp>
                  <id>factorial</id>
                  <ExprList>
                    <id>n</id>
                  </ExprList>
                </CallProcOp>
              </StatList>
            </DoOp>
          </WhileOp>
        </StatList>
        <ReturnExprs>null</ReturnExprs>
      </ProcBodyOp>
    </ProcOp>
  </ProcOpList>
</ProgramOp>
