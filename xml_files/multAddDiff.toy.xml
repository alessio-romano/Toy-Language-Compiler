<?xml version="1.0" encoding="UTF-8"?>
<ProgramOp>
  <VarDeclList>
    <VarDeclOp>
      <TypeOp>string</TypeOp>
      <IdListInitOp>
        <IdList>
          <SimpleAssignOp>
            <id>nome</id>
            <StringConst>Michele</StringConst>
          </SimpleAssignOp>
        </IdList>
      </IdListInitOp>
    </VarDeclOp>
  </VarDeclList>
  <ProcOpList>
    <ProcOp>
      <id>multAddDiff</id>
      <ParamDeclList>null</ParamDeclList>
      <ResultTypeList>
        <ResultTypeOp>int</ResultTypeOp>
        <ResultTypeOp>int</ResultTypeOp>
        <ResultTypeOp>int</ResultTypeOp>
      </ResultTypeList>
      <ProcBodyOp>
        <VarDeclList>
          <VarDeclOp>
            <TypeOp>int</TypeOp>
            <IdListInitOp>
              <IdList>
                <id>primo</id>
                <id>secondo</id>
                <id>mul</id>
                <id>add</id>
                <id>diff</id>
              </IdList>
            </IdListInitOp>
          </VarDeclOp>
        </VarDeclList>
        <StatList>
          <WriteOp>
            <ExprList>
              <StringConst>Inserire il primo argomento:</StringConst>
            </ExprList>
          </WriteOp>
          <ReadOp>
            <IdList>
              <id>primo</id>
            </IdList>
          </ReadOp>
          <WriteOp>
            <ExprList>
              <StringConst>Inserire il secondo argomento:</StringConst>
            </ExprList>
          </WriteOp>
          <ReadOp>
            <IdList>
              <id>secondo</id>
            </IdList>
          </ReadOp>
          <MultipleAssignOp>
            <IdList>
              <id>mul</id>
              <id>add</id>
              <id>diff</id>
            </IdList>
            <ExprList>
              <MultOp>
                <id>primo</id>
                <id>secondo</id>
              </MultOp>
              <PlusOp>
                <id>primo</id>
                <id>secondo</id>
              </PlusOp>
              <MinusOp>
                <id>primo</id>
                <id>secondo</id>
              </MinusOp>
            </ExprList>
          </MultipleAssignOp>
        </StatList>
        <ReturnExprs>
          <ExprList>
            <id>mul</id>
            <id>add</id>
            <id>diff</id>
          </ExprList>
        </ReturnExprs>
      </ProcBodyOp>
    </ProcOp>
    <ProcOp>
      <id>writeNewLines</id>
      <ParamDeclList>
        <ParamDeclOp>
          <TypeOp>int</TypeOp>
          <IdList>
            <id>n</id>
          </IdList>
        </ParamDeclOp>
      </ParamDeclList>
      <ResultTypeList>
        <ResultTypeOp>void</ResultTypeOp>
      </ResultTypeList>
      <ProcBodyOp>
        <VarDeclList>null</VarDeclList>
        <StatList>
          <WhileOp>
            <StatList>null</StatList>
            <GtOp>
              <id>n</id>
              <IntConst>0</IntConst>
            </GtOp>
            <DoOp>
              <StatList>
                <WriteOp>
                  <ExprList>
                    <StringConst />
                  </ExprList>
                </WriteOp>
                <MultipleAssignOp>
                  <IdList>
                    <id>n</id>
                  </IdList>
                  <ExprList>
                    <MinusOp>
                      <id>n</id>
                      <IntConst>1</IntConst>
                    </MinusOp>
                  </ExprList>
                </MultipleAssignOp>
              </StatList>
            </DoOp>
          </WhileOp>
        </StatList>
        <ReturnExprs>null</ReturnExprs>
      </ProcBodyOp>
    </ProcOp>
    <ProcOp>
      <id>main</id>
      <ParamDeclList>null</ParamDeclList>
      <ResultTypeList>
        <ResultTypeOp>void</ResultTypeOp>
      </ResultTypeList>
      <ProcBodyOp>
        <VarDeclList>
          <VarDeclOp>
            <TypeOp>int</TypeOp>
            <IdListInitOp>
              <IdList>
                <id>a</id>
                <id>b</id>
                <SimpleAssignOp>
                  <id>c</id>
                  <IntConst>0</IntConst>
                </SimpleAssignOp>
              </IdList>
            </IdListInitOp>
          </VarDeclOp>
        </VarDeclList>
        <StatList>
          <MultipleAssignOp>
            <IdList>
              <id>a</id>
              <id>b</id>
              <id>c</id>
            </IdList>
            <ExprList>
              <CallProcOp>
                <id>multAddDiff</id>
                <ExprList>null</ExprList>
              </CallProcOp>
            </ExprList>
          </MultipleAssignOp>
          <WriteOp>
            <ExprList>
              <StringConst>Ciao</StringConst>
              <id>nome</id>
            </ExprList>
          </WriteOp>
          <CallProcOp>
            <id>writeNewLines</id>
            <ExprList>
              <IntConst>2</IntConst>
            </ExprList>
          </CallProcOp>
          <WriteOp>
            <ExprList>
              <StringConst>I tuoi valori sono:</StringConst>
              <id>a</id>
              <StringConst>per la moltiplicazione</StringConst>
              <id>b</id>
              <StringConst>per la somma, e</StringConst>
              <id>c</id>
              <StringConst>per la differenza</StringConst>
            </ExprList>
          </WriteOp>
        </StatList>
        <ReturnExprs>null</ReturnExprs>
      </ProcBodyOp>
    </ProcOp>
  </ProcOpList>
</ProgramOp>
